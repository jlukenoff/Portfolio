{
  "name": "john-lukenoff-portfolio",
  "version": "1.0.0",
  "description": "Boilerplate for a full-stack MERN app",
  "main": "index.js",
  "scripts": {
    "start": "node public/server.js",
    "test": "jest",
    "dev:server": "nodemon --inspect public/server.js",
    "build": "NODE_ENV=production webpack -p --progress",
    "dev:build-server": "webpack --config server.webpack.config.js -w",
    "dev:client": "webpack-dev-server",
    "dev:build": "NODE_ENV=development webpack -w",
    "docker-compose": "docker run -d -p 8080:3000 -v $(pwd):/src/app --name mern-boilerplate jlukenoff/mern-boilerplate",
    "dev:functions": "firebase emulators:start --inspect-functions 9229 --only functions",
    "heroku-postbuild": "webpack -p",
    "firebase-deploy": "webpack -p && firebase deploy --only hosting"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/server/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/__test__/coverage/",
    "resolver": null,
    "testMatch": [
      "**/__test__/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlukenoff/MERN-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jlukenoff/MERN-boilerplate/issues"
  },
  "homepage": "https://github.com/jlukenoff/MERN-boilerplate#readme",
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@loadable/component": "^5.12.0",
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-emotion": "^10.0.9",
    "dotenv": "^8.2.0",
    "emotion": "^10.0.9",
    "emotion-server": "^10.0.9",
    "express": "^4.16.3",
    "firebase": "^7.14.2",
    "merge": "^1.2.1",
    "mongoose": "^5.9.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.8.6",
    "react-pdf": "^4.1.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.0.1",
    "supertest": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-airbnb": "^2.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.1",
    "fsevents": "^1.2.12",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
